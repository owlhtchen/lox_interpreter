fun world() {
    print ", world!";
}
fun hello() {
    print "hello";
    world();
}
fun greet(name) {
    print "hello, " + name;
}
fun bye(firstName, secondName) {
    print "goodbye, " + firstName + " " + secondName;
}
hello();
greet("chen");
