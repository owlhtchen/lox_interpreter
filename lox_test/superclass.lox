  class Doughnut {
    cook() {
      print "Dunk in the fryer.";
      this.finish("sprinkles");
    }

    finish(ingredient) {
      print "Finish with " + ingredient;
    }
  }
  class Cruller < Doughnut {
    finish(ingredient) {
      // No sprinkles.
      super.finish("icing");
    }
  }
  var cruller = Cruller();
  cruller.finish("unknown");
  cruller.cook();

